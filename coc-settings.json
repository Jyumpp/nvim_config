  // "yaml.trace.server": "verbose",
  "coc.preferences.enableLinkedEditing": true,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSave": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "coc.source.file.ignoreHidden": false,
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "colors.enable": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true
  },
  "diagnostic.format": "%message [%source]",
  "diagnostic.separateRelatedInformationAsDiagnostics": true,
  "diagnostic.virtualText": false,
  "hover.floatConfig": {
    "border": true,
    "rounded": true
  },
  "languageserver": {},
  "links.highlight": true,
  "links.tooltip": true,
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-n>": "do:previewdown",
    "<C-p>": "do:previewup"
  },
  "list.normalMappings": {
    "<C-c>": "do:exit",
    "<C-n>": "do:previewdown",
    "<C-p>": "do:previewup"
  },
  "notification.statusLineProgress": true,
  "pullDiagnostic.onChange": false,
  "pullDiagnostic.onSave": true,
  "pyright.organizeimports.provider": "ruff",
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.stubPath": "~/src/python-type-stubs",
  "python.formatting.blackdHTTPHeaders": {
    "X-Fast-Or-Safe": "fast"
  },
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.ruffEnabled": true,
  "semanticTokens.enable": true,
  "signature.floatConfig": {
    "border": true,
    "rounded": true
  },
  "signature.target": "echo",
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.defaultSortMethod": "none",
  "suggest.detailField": "abbr",
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "suggest.selection": "recentlyUsedByPrefix",
  "workspace.ignoredFolders": [
    "$HOME",
    "$HOME/.cargo/**",
    "$HOME/.rustup/**",
    "$HOME/pkg/mod/**",
    "$HOMEBREW_PREFIX",
    "$HOMEBREW_PREFIX/**"
  ],

  "diagnostic.errorSign": "✗",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.hintSign": "➤"
}
